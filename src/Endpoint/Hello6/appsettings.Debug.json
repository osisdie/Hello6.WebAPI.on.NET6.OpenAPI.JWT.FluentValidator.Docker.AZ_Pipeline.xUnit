{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "RedisInstances": {
    "ConnectionString": "127.0.0.1:6379"
  },
  "AuthConfig": {
    "JwtConfig": {
      "Secret": "{{your-secret-string}}",
      "AccessTokenExpireMins": 60,
      "RefreshTokenExpireMins": 1440
    }
  },
  "SDK": {
    "FeatureToggles": {
      "HelloSendCommand_Service_V2_Enabled": true
    }
  }
}
